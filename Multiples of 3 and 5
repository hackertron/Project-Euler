#include <stdio.h>
// C program for project euler problem 1 
int main() {
    int total = 0;
    for ( int i = 0; i < 1000; i++ ) {
        if ( i % 3 == 0 || i % 5 == 0 ) {
            printf( " %d ", i );
            total = total + i;
        }
    }
    printf( "\nTotal: %d\n", total );
    return 0;
} 
